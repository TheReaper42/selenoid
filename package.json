{
  "name": "fjhk",
  "version": "1.0.1",
  "description": "e2e-test for CreditOnline applications",
  "main": "index.js",
  "scripts": {
    "test:selenoid": "cross-env TESTS_ENV=ui npm run config && ./node_modules/.bin/wdio config/wdio.selenoid.conf.js",
    "test:win_selenoid": "cross-env TESTS_ENV=ui npx wdio ./config/wdio.selenoid.conf.js",
    "test:chrome-selenoid": "cross-env TESTS_ENV=ui npm run config && ./node_modules/.bin/wdio config/wdio.chrome.selenoid.conf.js",
    "test:win_chrome-selenoid": "cross-env TESTS_ENV=ui  npx wdio config/wdio.chrome.selenoid.conf.js",
    "config": "sh -ac 'if test \"$TESTS_ENV\"; then cp environments/env.${TESTS_ENV}.json.template environments/env.json; fi'",
    "allire:clean": "rm -rf allure-results/* allure-report/*",
    "allure:history": "cp -R allure-report/history allure-results",
    "allure:serve": "npx allure serve allure-results",
    "allure:serve:hi": "npm run allure:history && npx allure serve allure-results",
    "allure:generate:hi": "npm run allure:history && npx allure generate allure-results --clean",
    "allure:generate": "npx allure generate allure-results --clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@wdio/allure-reporter": "^7.7.3",
    "@wdio/cli": "^7.7.4",
    "@wdio/firefox-profile-service": "^7.7.3",
    "@wdio/local-runner": "^7.16.12",
    "@wdio/mocha-framework": "^7.16.11",
    "@wdio/selenium-standalone-service": "^7.7.4",
    "@wdio/spec-reporter": "^7.16.11",
    "chai": "^4.3.4",
    "chromedriver": "^91.0.1",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "path": "^0.12.7",
    "wdio-chromedriver-service": "^7.2.6"
  },
  "dependencies": {
    "allure-commandline": "^2.13.8",
    "expect": "^27.0.6",
    "expect-webdriverio": "^3.1.0",
    "fs": "0.0.1-security",
    "hosted-git-info": ">=2.8.9",
    "npm": "^7.23.0",
    "shortid": "^2.2.16",
    "wdio-selenoid-video-reporter": "^1.0.7",
    "webdriverio": "^7.7.4"
  }
}
